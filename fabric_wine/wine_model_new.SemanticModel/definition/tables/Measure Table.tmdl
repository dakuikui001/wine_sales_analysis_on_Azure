	measure 'Total Cost' = SUMX(processed_sales, processed_sales[Quantity]*RELATED('product'[DealerPrice]))
		lineageTag: 3d57bc8c-8f1e-4353-b88f-e5eeb9b866de

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Previous Month Orders' = ```
			
			VAR CurrentMonthDate = SELECTEDVALUE('dates'[LastDayOfMonth])
			VAR PreviousMonthDate = 
			    MAXX(
			        FILTER(
			            ALL('dates'[LastDayOfMonth]), 
			            'dates'[LastDayOfMonth] < CurrentMonthDate
			        ), 
			        'dates'[LastDayOfMonth]
			    )
			RETURN
			CALCULATE(
			    [Total Orders],
			    ALL('dates'),
			    'dates'[LastDayOfMonth] = PreviousMonthDate
			)
			```
		formatString: 0
		lineageTag: 4ab9592b-91be-4db7-a785-5443e5acc1d1

	measure 'Gross Profit' = [Total Sales] - [Total Cost]
		lineageTag: a580cd43-5b6a-49b4-b65f-77e889a7b3d9

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Gross Margin %' = DIVIDE([Gross Profit], [Total Sales], 0)
