	measure 'Total Sales' = SUMX(processed_sales, processed_sales[ListPrice] * processed_sales[Quantity])
		lineageTag: 06253b26-a3a0-425f-954a-36d5484ebc66

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Total Orders' = SUM(processed_sales[Quantity])
