						"source(output(",
						"          FromCurrency as string,",
						"          ToCurrency as string,",
						"          EffectiveDate as date,",
						"          AverageRate as double,",
						"          load_timestamp as timestamp",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
