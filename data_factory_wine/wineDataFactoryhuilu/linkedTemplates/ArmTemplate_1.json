					"scriptLines": [
						"source(output(",
						"          ProductId as integer,",
						"          Country as string,",
						"          Score as short,",
						"          DealerPrice as short,",
						"          Markup as double,",
						"          ListPrice as double,",
						"          Province as string,",
						"          Region_1 as string,",
						"          Region_2 as string,",
						"          Title as string,",
						"          Vintage as short,",
						"          Variety as string,",
						"          Winery as string,",
						"          Year as short,",
						"          website as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: true,",
						"     purgeFiles: true,",
						"     partitionRootPath: 'medallion/bronze/product_parquet',",
						"     format: 'parquet') ~> CopyParquetProduct",
						"CopyParquetProduct derive(load_timestamp = currentUTC()) ~> derivedColumn1",
						"filter1 select(mapColumn(",
						"          ProductId,",
						"          Country,",
						"          Score,",
						"          DealerPrice,",
						"          Markup,",
						"          ListPrice,",
						"          Province,",
						"          Region_1,",
						"          Region_2,",
						"          Title,",
						"          Vintage,",
						"          Variety,",
						"          Winery,",
						"          Year,",
						"          Store = website,",
						"          load_timestamp",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> SelectColumns",
